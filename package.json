{
  "name": "electron-typescript-react-tailwind-redux",
  "productName": "electron-typescript-react-tailwind-redux",
  "version": "2.0.0",
  "description": "An electron boilerplate :)",
  "main": ".webpack/main",
  "scripts": {
    "test": "jest",
    "start": "electron-forge start",
    "format": "prettier --ignore-path --write",
    "prepare": "husky install .config/husky",
    "commit": "cz",
    "create-icons": "electron-icon-builder --input=./assets/package/source.png --output=./assets/package",
    "package:mac": "electron-forge package --platform darwin",
    "package:linux": "electron-forge package --platform linux",
    "package:win": "electron-forge package --platform win32",
    "make:mac": "electron-forge make --platform darwin",
    "make:linux": "electron-forge make --platform linux",
    "make:win": "electron-forge make --platform win32",
    "publish:mac": "electron-forge publish --platform darwin",
    "publish:linux": "electron-forge publish --platform linux",
    "publish:win": "electron-forge publish --platform win32"
  },
  "keywords": [],
  "author": {
    "name": "daniel",
    "email": "saucesteals@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./.config/forge.config.js",
    "commitizen": {
      "path": "cz-conventional-changelog",
      "defaultScope": "global"
    }
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": false
  },
  "build": {
    "extraResources": [
      {
        "from": "./src/extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.21.4",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^29.5.0",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "dotenv": "^16.0.0",
    "electron": "17.1.0",
    "electron-icon-builder": "^2.0.1",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.7",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "crypto-browserify": "^3.12.0",
    "daisyui": "^2.51.5",
    "electron-is-dev": "^2.0.0",
    "electron-persist-secure": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux-persist": "^6.0.0"
  }
}
